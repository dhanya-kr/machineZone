{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["js"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "theme": "simplex"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "build/docs/",
    "recurse": true,
    "readme": "README.md"
  }
}
