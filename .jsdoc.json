{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include":  ["js/"],
        "includePattern": ".+\\.js(doc)?$" ,
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
  "opts": {
    "encoding": "utf8",
    "destination": "build/docs/",
    "recurse": true,
    "readme": "README.md"
  }
}
